.divLine {
  display: flex;
  flex-direction: column;
  height: 20vh;
  border-radius: 20px;
  border: 5rpx solid #1691f5;
}

.text1 {
  color: rgb(1, 81, 253);
  font-weight: bold;
  font-size: 40rpx;
  text-align: center;
}

.title1 {
  font-weight: 600;
  margin-left: 20rpx;
  margin-right: 100rpx;
  flex: 1;
  font-size: 30rpx;
}

.about-banner {
  width: 75rpx;
  height: 75rpx;
  right: 0;
  margin-right: 15px;
  border-radius: 10px;
  border: 5rpx solid #1691f5;
  margin-top: 10px;
}

.setting {
  height: 2vh;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 80rpx;
}